{
  "name": "rj-labs",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo dev",
    "dev:auth": "pnpm dev --filter=auth-app --filter=auth-service",
    "dev:docs": "pnpm dev --filter=docs",
    "dev:web": "pnpm dev --filter=web",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo check-types",
    "clean": "turbo clean"
  },
  "dependencies": {
    "turbo": "^2.6.1"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}